<h1>Listing tpousers</h1>

<table>
  <tr>
    <th>V</th>
    <th>Userid</th>
    <th>Profileid</th>
    <th>Day</th>
    <th>New</th>
    <th>Total</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tpousers.each do |tpouser| %>
  <tr>
    <td><%= tpouser.v %></td>
    <td><%= tpouser.UserId %></td>
    <td><%= tpouser.ProfileId %></td>
    <td><%= tpouser.Day %></td>
    <td><%= tpouser.New %></td>
    <td><%= tpouser.Total %></td>
    <td><%= link_to 'Show', tpouser %></td>
    <td><%= link_to 'Edit', edit_tpouser_path(tpouser) %></td>
    <td><%= link_to 'Destroy', tpouser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

